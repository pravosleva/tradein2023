    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.5",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.3.5",
    "terser": "^5.26.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-preload": "^0.3.1",
    "vite-plugin-pwa": "^0.20.0"
  }
